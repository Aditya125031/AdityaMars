#include <stdio.h>
#include <string.h>
#include <ctype.h>
void decode(char code[])
{
    int temp;
    for(int i=0;code[i]!='\0';i++)
    {
        code[i] = toupper(code[i]);
        temp=(int)code[i];
        printf("%c",(char)(temp-i-1));
    }
}
int main()
{
    char code[100];
    scanf("%s",code);
    decode(code);
}
